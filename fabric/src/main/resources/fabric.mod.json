{
  "schemaVersion": 1,
  "id": "breadmod",
  "version": "${version}",
  "name": "Bread Mod",
  "description": "the breadmod...",
  "authors": [
    "2!!!"
  ],
  "contact": {
    "homepage": ""
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/breadmod/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      {
        "adapter": "kotlin",
        "value": "bread.mod.breadmod.fabric.datagen.ModDataGenerator"
      }
    ],
    "main": [
      {
        "adapter": "kotlin",
        "value": "bread.mod.breadmod.fabric.MainFabric"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "bread.mod.breadmod.fabric.client.MainFabricClient"
      }
    ],
    "modmenu": [
      {
        "adapter": "kotlin",
        "value": "bread.mod.breadmod.fabric.ModMenuIntegration"
      }
    ],
    "jade": [
      "bread.mod.breadmod.client.waila.jade.JadePlugin"
    ]
  },
  "mixins": [
    "breadmod.mixins.json"
  ],
  "accessWidener": "breadmod.accesswidener",
  "depends": {
    "minecraft": "~1.21.1",
    "java": ">=21",
    "architectury": ">=13.0.6",
    "fabric-api": "*",
    "fabricloader": ">=0.16.4",
    "fabric-language-kotlin": ">=1.12.1+kotlin.2.0.20"
  },
  "suggests": {},
  "custom": {
    "modmenu": {
      "update_checker": false
    }
  }
}
